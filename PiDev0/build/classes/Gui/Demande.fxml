<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox fx:id="HBoxDemnade" prefHeight="56.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DemandeController">
   <children>
      <Pane prefHeight="50.0" prefWidth="50.0" />
      <Label fx:id="lbDateDemande" prefHeight="56.0" prefWidth="115.0" text="Date Demande" />
      <Label fx:id="lbUtilisateur" prefHeight="63.0" prefWidth="104.0" text="Utilisateur" />
      <Label fx:id="lbDestination" prefHeight="63.0" prefWidth="104.0" text="Destination" />
      <Label fx:id="lbLibelleEvenement" prefHeight="56.0" prefWidth="121.0" text="LibelleEvenement" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onMousePressed="#click" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: #EBC95E;" styleClass="add-btn" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/vue%20(1).png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Pane prefHeight="56.0" prefWidth="13.0" />
            <Button mnemonicParsing="false" onAction="#SupprimerDemenedeEvenement" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: #E37A7A;" styleClass="add-btn" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/supprimer.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
      </HBox>
   </children>
</HBox>
